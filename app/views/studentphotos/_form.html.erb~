<%= form_for(@studentphoto,:html => {:multipart => true}) do |f| %>
  <% if @studentphoto.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@studentphoto.errors.count, "error") %> prohibited this studentphoto from being saved:</h2>

      <ul>
      <% @studentphoto.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :student_id %><br />
    <%= f.number_field :student_id %><br />
    <%= f.file_field :photo %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
